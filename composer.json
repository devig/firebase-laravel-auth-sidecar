{
    "name": "devig/firebase-laravel-auth-sidecar",
    "description": "Firebase Auth Add-on for Laravel",
    "keywords": ["firebase", "auth", "authentication", "laravel", "gbrits"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Grant Brits",
            "email": "grant@maximus.agency",
            "homepage": "https://maximus.agency"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "*",
        "illuminate/contracts": "*",
        "firebase/php-jwt": "^5.0"
